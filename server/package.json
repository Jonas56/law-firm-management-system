{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose --runInBand",
    "build:ui": "rm -rf build && cd ../web && npm run build --prod && cp -r build ../server",
    "deploy": "git push heroku master",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "migrate": "sequelize-cli db:migrate --from 20211023211352-create-case.js --to 20211024171700-create-client.js && sequelize-cli db:migrate",
    "migrate:undo:all": "sequelize-cli db:migrate:undo --name 20211023211352-create-case.js && sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed --seed 20211029161101-demo-user.js  && sequelize-cli db:seed --seed 20211029154655-demo-client.js && sequelize-cli db:seed --seed 20211029153645-demo-case.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "mysql2": "^2.3.2",
    "sequelize": "^6.7.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "jest": "^27.1.1",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.6"
  }
}
